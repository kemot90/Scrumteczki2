<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.kemot.scrum.scrumteczki2"
    android:versionCode="1"
    android:versionName="0.0.2" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="16" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:name=".ScrumteczkiApp"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.EstimateActivity" />
        <activity android:name=".activity.SprintListActivity" />
        <activity android:name=".activity.DailyScrumActivity" />
        <activity android:name=".activity.BreakActivity" />
        <activity android:name=".activity.UnknownEstimateActivity" />
    </application>

</manifest>
